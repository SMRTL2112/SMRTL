Select
'SDTL' Facility_Name,
 '46D2183484' Facility_CLIA,
'10644 South Jordan Gateway Suite 200' Facility_Street_Address,
'South Jordan' Facility_City, 
'UT' Facility_State,
'84085' Facility_Zip,
'8019949454' Facility_phone,
a.PATIENT_ID  Patient_Identifier,
a.patient_first_name  First_Name,
a.PATIENT_LAST_NAME  Last_Name,
a.PATIENT_MIDDLE_NAME Middle_name,
a.PATIENT_DOB  DOB,
DECODE(a.PATIENT_GENDER, 'Male', 'M', 'Femle', 'F') Patient_Sex, 
DECODE(a.PATIENT_RACE , 'Black', 'Black / African-American','White', 'White', 'African American or Black','Black / African-American'
,'White (Caucasian)', 'White', 'WHITE', 'White', 'Japanese', 'Asian','Asian Indian', 'Asian', 'Unknown') Race,
DECODE(a.PATIENT_ETHNICITY, 'Not of Hispanic, Latino or Spanish Origin', 'Not Hispanic or Latino','Non-Hispanic', 
'Not Hispanic or Latino','Not of Hispanic, Latino or Spanish Origin', 'Other','Hispanic, Latino or Spanish Origin', 
'Hispanic or Latino', 'Not of Hispanic, Latino(a), or Spanish Origin', 'Not Hispanic or Latino', 'Unknown') Ethnicity, 
a.PATIENT_CITY  Patient_City,
a.PATIENT_STATE  Patient_State,
a.PATIENT_ZIP_CODE  Patient_Zip,
a.PATIENT_COUNTY  PATIENT_COUNTY,
a.PATIENT_PHONE_NUMBER  Patient_Home_Phone,
''  OKtoContactPatient,
a.PATIENT_ID MRN,
a.ORDERING_PROVIDER_FIRST_NAME  Provider_first_Name,
a.ORDERING_PROVIDER_LAST_NAME  Provider_last_name,
a.ORDERING_PROVIDER_STREET  ORDERING_PROVIDER_ADDRESS,
a.ORDERING_PROVIDER_CITY  ORDERING_PROVIDER_CITY,
a.ORDERING_PROVIDER_STATE  ORDERING_PROVIDER_STATE,
a.ORDERING_PROVIDER_ZIP_CODE  ORDERING_PROVIDER_ZIP_CODE,
'Novel Coronavirus (SARS-CoV-2)'  Disease,
a.TESTING_LAB_ACCESSION_NUMBER  Accession_Number,
a.SPECIMEN_COLLECTION_DATE_TIME  Specimen_collection_date,
a.SPECIMEN_TYPE_CODE  Specimen_Type,
'Spectrum Solutions SDNA-1000'  MANUFACTURER_AND_TEST_KIT_NAME,
a.TEST_RESULT_DESCRIPTION  TEST_RESULT,
a.test_date report_date,
(Select Sysdate as Message_Date_Time from dual) Result_Reported_Date,
a.ORDERING_PROVIDER_FIRST_NAME || a.ORDERING_PROVIDER_LAST_NAME ordering_provider_name,
'SDTL'  Performing_Facility_ID
from aims a
where patient_state in ('CA', 'CALIFORNIA')
and a.PATIENT_COUNTY = 'San Diego'
and a.BATCH_ID is null;
